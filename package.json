{
  "name": "investigations-client",
  "author": "Rubin EPO",
  "version": "2.0.0-alpha",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/investigations-client"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "next dev",
    "static": "npm-run-all static:*",
    "static:build": "next build --debug",
    "static:export": "next export",
    "start": "next start -p 8080",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint -c .eslintrc.js \"**/*.{ts,js,jsx,tsx}\"",
    "lint:scss": "stylelint --config .stylelintrc.js \"**/*.scss\"",
    "fix": "npm-run-all fix:js fix:scss",
    "fix:js": "eslint -c .eslintrc.js \"**/*.{ts,js,jsx,tsx}\" --fix",
    "fix:scss": "stylelint --config .stylelintrc.js \"**/*.scss\" --fix",
    "fix:staged": "lint-staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{js,jsx}": "yarn fix:js",
    "*.scss": "yarn fix:scss"
  },
  "dependencies": {
    "@castiron/style-mixins": "^1.0.6",
    "@rubin-epo/epo-react-lib": "^1.2.5",
    "@rubin-epo/epo-widget-lib": "^0.2.1",
    "classnames": "^2.3.2",
    "focus-trap": "^7.3.1",
    "focus-visible": "^5.2.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^22.4.10",
    "lodash": "^4.17.21",
    "next": "^13.2.3",
    "next-build-id": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-is": "^18.2.0",
    "react-uid": "^2.3.2",
    "screenfull": "^6.0.2",
    "styled-components": "^6.0.0-beta.14",
    "swr": "1.3.0",
    "universal-cookie": "^4.0.4",
    "use-resize-observer": "^9.1.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/nextjs": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/testing-library": "^0.1.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-plugin-styled-components": "^2.0.3",
    "browserslist": "^4.21.5",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unused-imports": "^2.0.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.6",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "storybook": "^7.0.9",
    "storybook-react-i18next": "^2.0.1",
    "stylelint": "^14.5.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "4.9.5"
  }
}
